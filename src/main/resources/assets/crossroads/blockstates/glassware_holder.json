{
	"multipart": [
		{
			"when": { "container_type": "none|phial" },
			"apply": { "model": "crossroads:block/glassware_stand_base_phial" }
		},
		{
			"when": { "container_type": "phial", "crystal": "false" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_phial_glass" }
		},
		{
			"when": { "container_type": "phial", "crystal": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_phial_cryst" }
		},

		{
			"when": { "container_type": "florence" },
			"apply": { "model": "crossroads:block/glassware_stand_base_florence" }
		},
		{
			"when": { "container_type": "florence", "crystal": "false" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_florence_glass" }
		},
		{
			"when": { "container_type": "florence", "crystal": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_florence_cryst" }
		},

		{
			"when": { "container_type": "shell" },
			"apply": { "model": "crossroads:block/glassware_stand_base_shell" }
		},
		{
			"when": { "container_type": "shell", "crystal": "false" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_shell_glass" }
		},
		{
			"when": { "container_type": "shell", "crystal": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_shell_cryst" }
		}
	]
}
