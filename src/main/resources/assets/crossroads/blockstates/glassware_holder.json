		{
			"when": { "container_type": "florence", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_base_florence", "x": 180 }
		},
		{
			"when": { "container_type": "florence", "crystal": "false", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_florence_glass", "x": 180 }
		},
		{
			"when": { "container_type": "florence", "crystal": "true", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_florence_cryst", "x": 180 }
		},
		{
			"when": { "container_type": "shell", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_base_shell", "x": 180 }
