			"apply": { "model": "crossroads:block/glassware_stand_bottle_shell_cryst" }
		},

		{
			"when": { "container_type": "none|phial", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_base_phial", "x": 180 }
		},
		{
			"when": { "container_type": "phial", "crystal": "false", "inverted": "true" },
			"apply": { "model": "crossroads:block/glassware_stand_bottle_phial_glass", "x": 180 }
