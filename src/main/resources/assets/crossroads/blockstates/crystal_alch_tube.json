{
	"multipart": [
		{
			"apply": { "model": "crossroads:block/conduit/fluid_tube_core" }
		},

		{   "when": { "up_b": "true", "up": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in" }
		},
		{   "when": { "up_b": "true", "up": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out" }
		},

		{   "when": { "down_b": "true", "down": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in", "x": 180, "y": 0 }
		},
		{   "when": { "down_b": "true", "down": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out", "x": 180, "y": 0 }
		},

		{   "when": { "east_b": "true", "east": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in", "x": 90, "y": 90 }
		},
		{   "when": { "east_b": "true", "east": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out", "x": 90, "y": 90 }
		},

		{   "when": { "west_b": "true", "west": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in", "x": 90, "y": 270 }
		},
		{   "when": { "west_b": "true", "west": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out", "x": 90, "y": 270 }
		},

		{   "when": { "north_b": "true", "north": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in", "x": 90, "y": 0 }
		},
		{   "when": { "north_b": "true", "north": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out", "x": 90, "y": 0 }
		},

		{   "when": { "south_b": "true", "south": "input" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_in", "x": 90, "y": 180 }
		},
		{   "when": { "south_b": "true", "south": "output" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_cryst_out", "x": 90, "y": 180 }
		}
	]
}
