{
	"multipart": [
		{
			"when": { "active": "true" },
			"apply": { "model": "crossroads:block/reagent_pump_core" }
		},
		{
			"when": { "active": "false" },
			"apply": { "model": "crossroads:block/reagent_pump_core", "x": 180 }
		},
		{
			"when": { "active": "true" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_out" }
		},
		{
			"when": { "active": "false" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_out", "x": 180 }
		},
		{
			"when": { "north_b": "true" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_in", "x": 90 }
		},
		{
			"when": { "south_b": "true" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_in", "x": 90, "y": 180 }
		},
		{
			"when": { "east_b": "true" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_in", "x": 90, "y": 90 }
		},
		{
			"when": { "west_b": "true" },
			"apply": { "model": "crossroads:block/conduit/alch_tube_glass_in", "x": 90, "y": 270 }
		}
	]
}
